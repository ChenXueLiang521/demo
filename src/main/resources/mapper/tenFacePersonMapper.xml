<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tencentcloudapi.dao.TenFacePersonMapper">

    <insert id="insertPerson" parameterType="com.tencentcloudapi.model.TenFacePerson">
        INSERT INTO  ten_face_person (groupId,personId,gender,group_state,person_state,times,faceId)
        VALUES (#{groupId},#{personId},#{gender},#{group_state},#{person_state},#{times},#{faceId})
    </insert>

</mapper>